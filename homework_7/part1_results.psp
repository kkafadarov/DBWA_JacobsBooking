cursor.execute ("SELECT * FROM User u GROUP BY (u.userId IN (SELECT bb.userId FROM BookedBy bb))")
req.write("<table class=\"table table-hover\">")
req.write("<thead><tr><td>First Name</td><td>Last Name</td><td>Email</td></td></thead>")
req.write("<tbody>")
for row in cursor.fetchall():
  req.write("<tr>")
  req.write("<td>" + str(row[0]) + "</td>")
  req.write("<td>" + str(row[1]) + "</td>")
  req.write("<td>" + str(row[5]) + "</td>")
  req.write("<td><form action=\"part1_detailed.psp\" method=\"post\">")
  req.write("<input type=\"hidden\" name=\"userId\" value=\"%s\">" % str(row[7]))
  req.write("<button class=\"btn btn-primary\" type=\"submit\">Details</button></div></form></td>")
  req.write("</tr>")

req.write("</tbody></table>")
conn.commit()
conn.close()
